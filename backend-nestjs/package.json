{
  "name": "noor-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "prestart:prod": "npm run build",
    "start:prod": "node dist/main.js",
    "seed:dev": "NODE_ENV=production node --require ts-node/register ./scripts/seed-dev.ts",
    "migration:run": "NODE_ENV=production node --require ts-node/register ./node_modules/typeorm/cli.js migration:run -d ormconfig.ts",
    "seed:dev": "NODE_ENV=production node --require ts-node/register ./scripts/seed-dev.ts",
    "postbuild": "node ./scripts/verify-dist-entry.js"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.4.20",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^10.4.20",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ioredis": "^5.3.2",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "typeorm": "^0.3.27",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^6.0.0",
    "@types/passport-jwt": "^4.0.1",
    "rimraf": "^5.0.7",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
